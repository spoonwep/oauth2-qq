{
    "name": "spoonwep/oauth2-qq",
    "description": "QQ OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "spoonwep",
            "email": "i@sell.moe",
            "homepage": "http://sell.moe"
        }
    ],
    "keywords": [
      "oauth",
      "oauth2",
      "client",
      "authorization",
      "authorisation",
      "qq"
    ],
    "require": {
        "php": ">=5.4.0",
        "league/oauth2-client": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "spoonwep\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "spoonwep\\OAuth2\\Client\\Test\\": "tests/src/"
      }
    },
    "extra": {
      "branch-alias": {
        "dev-master": "1.0.x-dev"
      }
    }
}
